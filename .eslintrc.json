{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "parser": "flow"
      }
    ],
    // "singleQuote": true,
    "no-unused-vars": "error",
    "no-console": "warn"
  }
}
